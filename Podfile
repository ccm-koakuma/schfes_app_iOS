target 'schfes_app_iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod "SwiftyJSON"
  pod "Alamofire", "~> 4.4"
  pod "AlamofireImage"
  pod "PagingMenuController"
  pod "Fabric"
  pod "TwitterKit"

  # Pods for schfes_app_iOS

  target 'schfes_app_iOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'schfes_app_iOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
